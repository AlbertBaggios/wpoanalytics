\end_layout

\begin_layout Standard
\begin_inset Formula \[
RP[i_{1},\text{â€¦},i_{d}]=\sum_{a_{1}=b_{1}}^{b_{1}+k}\text{â€¦}\sum_{a_{d}=b_{d}}^{b_{d}+k}A[a_{1},\text{â€¦},a_{d}]\]

\end_inset


\end_layout

